# # # # # # # # # # # def test(a,b):
# # # # # # # # # # # 	z=a*b
# # # # # # # # # # # 	x=a/b
# # # # # # # # # # # 	return z,x

# # # # # # # # # # # # q,w=test(5,7)
# # # # # # # # # # # # print(q)
# # # # # # # # # # # # print(w)
# # # # # # # # # # # print(z)	#變數的值裡外不共通【僅限於變數，儲存記憶體代表指向特定位置(多個值)】

# # # # # # # # # # y=100

# # # # # # # # # # def test(a=5,b=3): #可於參數後方設定預設值
# # # # # # # # # # 	global y
# # # # # # # # # # 	z=a*b
# # # # # # # # # # 	x=a/b
# # # # # # # # # # 	return z,x

# # # # # # # # # # q,w=test(b=7,a=10) 
# # # # # # # # # # #當參數有預設值可不填參數值執行，系統將代入預設值
# # # # # # # # # # #當多個參數有預設值，同上可無須填寫參數值執行
# # # # # # # # # # #當有預設值情況下設定參數，將無條件取代原先預設值執行
# # # # # # # # # # print(q)
# # # # # # # # # # print(w)

# # # # # # # # # x=10
