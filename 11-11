# # # # # 	print(v)


# # # # #break/continue用法

# # # # # i=0
# # # # # v=0
# # # # # while i <1000:
# # # # # 	i+=1
# # # # # 	if i==500:
# # # # # 		 break
# # # # # 	v+=i
# # # # # print("i=",i)
# # # # # print("v=",v)

# # # # i=0
# # # # v=0
# # # # while i <1000:
# # # # 	i+=1
# # # # 	if i==500:
# # # # 		 continue
# # # # 	v+=i
# # # # print("i=",i)
# # # # print("v=",v)

# # # # #break用於直接中斷程式，continue用於跳過特定條件的值

# # # # def test(a):
# # # # 	try:
# # # # 		return a/10
# # # # 	except:
# # # # 		return"參數錯誤"
# # # # print(test(100))
# # # # print(test("ABC"))

# # # # class aa:
# # # # 	x=10
# # # # 	def test(self,v): #所有的:後沒東西都要打PASS不然會出錯
# # # # 		return self.x+v

# # # # class bb:
# # # # 	x=20
# # # # 	def test(self,v):
# # # # 		return self.x+v

# # # # print(a.test(10))#有使用self此呼叫方式無法使用

# # # # a=aa()
# # # # print(a.test(10))#沒使用self此呼叫方式無法使用

# # # # #差別在有無使用類別名稱【self】，宣告方式差異性

# # # # b=bb()
# # # # print(b.test(20))

# # # class aa:
# # # 	x=10
# # # 	@staticmethod
# # # 	def test(v): #所有的:後沒東西都要打PASS不然會出錯
# # # 		return v
# # # a=aa.test
# # # print(aa)

# # # class bb:
# # # 	x=20
# # # 	def __init__(self):
# # # 		self.x+=30
# # # 	def test(self,v):
# # # 		return self.x+v
# # # b=bb()
# # # b.x=30 #如下呼叫方式需要先將類別轉為變數才能進行呼叫
# # # print(b.test(20))#類別變數呼叫【self對應此呼叫方式】

# # # print(bb.test(20))#變數呼叫

# # #@staticmethod 類別變數中不可使用self,直接使用變數即可【可使用多個變數】
# # #@classmethod  第一個參數將無條件指向類別本身，需要透過類別變數轉換後才可以呼叫

