# # # # # # 	v+=i
# # # # # # print(v)

# # # # # #串列無須轉換即可使用迴圈讀取執行，若要顯示串列內容則需要轉為list

# # # # # i=0
# # # # # v=0
# # # # # while i <1000:
# # # # # 	i=i+1
